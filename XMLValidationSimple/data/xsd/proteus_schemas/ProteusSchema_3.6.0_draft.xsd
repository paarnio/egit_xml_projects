<xsd:schema xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsd="http://www.w3.org/2001/XMLSchema" version="1.0">
	<xsd:annotation>
		<xsd:documentation>Schema Name: XMpLant</xsd:documentation>
	</xsd:annotation>
	<!-- ItemAttribute definitions - these relate to engineering attributes of Plant Items -->
	<!-- Basic Attributes -->
	<xsd:attribute name="Format">
		<xsd:simpleType>
			<xsd:restriction base="xsd:NMTOKEN">
				<xsd:enumeration value="string"/>
				<xsd:enumeration value="byte"/>
				<xsd:enumeration value="short"/>
				<xsd:enumeration value="integer"/>
				<xsd:enumeration value="long"/>
				<xsd:enumeration value="ulong"/>
				<xsd:enumeration value="float"/>
				<xsd:enumeration value="double"/>
				<xsd:enumeration value="anyURI"/>
			</xsd:restriction>
		</xsd:simpleType>
	</xsd:attribute>
	<xsd:attribute name="Units" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>This reference the EPISTLE Reference Data Library (ERDL) Units of measure </xsd:documentation>
		</xsd:annotation>
	</xsd:attribute>
	<!-- Basic types -->
	<xsd:complexType name="Integer">
		<xsd:attribute name="Value" type="xsd:integer" use="optional"/>
		<xsd:attribute ref="Units" use="optional"/>
		<xsd:attribute name="ValueURI" type="xsd:anyURI" use="optional"/>
		<xsd:attribute name="UnitsURI" type="xsd:anyURI" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="Double">
		<xsd:attribute name="Value" type="xsd:double" use="optional"/>
		<xsd:attribute ref="Units" use="optional"/>
		<xsd:attribute name="ValueURI" type="xsd:anyURI" use="optional"/>
		<xsd:attribute name="UnitsURI" type="xsd:anyURI" use="optional"/>
	</xsd:complexType>
	<xsd:complexType name="String">
		<xsd:attribute name="Value" type="xsd:string" use="optional"/>
		<xsd:attribute ref="Units" use="optional"/>
		<xsd:attribute name="ValueURI" type="xsd:anyURI" use="optional"/>
		<xsd:attribute name="UnitsURI" type="xsd:anyURI" use="optional"/>
	</xsd:complexType>
	<!--"Curve" Type  -->
	<xsd:complexType name="Curve"/>
	<!-- UV coordinate for Bsplines -->
	<xsd:complexType name="UVCoordinate">
		<xsd:attribute name="U" type="xsd:double" use="required"/>
		<xsd:attribute name="V" type="xsd:double" use="required"/>
	</xsd:complexType>
	<!-- UV Parametric curve for Bsplines -->
	<xsd:complexType name="Pcurve">
		<xsd:annotation>
			<xsd:documentation>A Pcurve is a 2D parametric curve defined in the UV space of the Surface to Which it belongs.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="Curve">
				<xsd:choice>
					<xsd:element name="UVCoordinate" type="UVCoordinate" minOccurs="3" maxOccurs="unbounded"/>
				</xsd:choice>
				<xsd:attribute name="NumPoints" type="xsd:nonNegativeInteger" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="Pcurve" type="Pcurve" substitutionGroup="Curve"/>
	<!-- List of Control Points -->
	<xsd:complexType name="ControlPoints">
		<xsd:sequence>
			<xsd:element ref="Coordinate" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- List of list of control points -->
	<xsd:complexType name="ControlPointArray">
		<xsd:sequence>
			<xsd:element name="ControlPoints" type="ControlPoints" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- List of Knot Multiplicities -->
	<xsd:complexType name="KnotMultiplicities">
		<xsd:sequence>
			<xsd:element name="Multiplicity" type="xsd:nonNegativeInteger" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- List of Knot location in parametric space -->
	<xsd:complexType name="Knots">
		<xsd:sequence>
			<xsd:element name="Knot" maxOccurs="unbounded">
				<xsd:simpleType>
					<xsd:restriction base="xsd:double">
						<xsd:minInclusive value="0.0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- List of Weights - one per control point for Rational curves and surfaces -->
	<xsd:complexType name="WeightsData">
		<xsd:sequence>
			<xsd:element ref="ControlPointWeight" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- List of WeightsData - a 2d array ( WeightsData is a an array) -->
	<xsd:complexType name="Weights">
		<xsd:sequence>
			<xsd:element name="WeightsData" type="WeightsData" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="TransactionType">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="to approve"/>
			<xsd:enumeration value="to check"/>
			<xsd:enumeration value="to copy"/>
			<xsd:enumeration value="to create"/>
			<xsd:enumeration value="to modify"/>
			<xsd:enumeration value="to request"/>
			<xsd:enumeration value="to release"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LengthUnitsType">
		<xsd:annotation>
			<xsd:documentation>These Length units are from the ISO 15926 Reference Data Library (RDL) Units of measure</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="mm"/>
			<xsd:enumeration value="cm"/>
			<xsd:enumeration value="dm"/>
			<xsd:enumeration value="m"/>
			<xsd:enumeration value="km"/>
			<xsd:enumeration value="in"/>
			<xsd:enumeration value="ft"/>
			<xsd:enumeration value="yd"/>
			<xsd:enumeration value="Millimetre"/>
			<xsd:enumeration value="Centimetre"/>
			<xsd:enumeration value="Decimetre"/>
			<xsd:enumeration value="Metre"/>
			<xsd:enumeration value="Kilometre"/>
			<xsd:enumeration value="Inch"/>
			<xsd:enumeration value="Yard"/>
			<xsd:enumeration value="Mile"/>
			<xsd:enumeration value="Foot"/>
			<xsd:enumeration value="Picometre"/>
			<xsd:enumeration value="Micrometre"/>
			<xsd:enumeration value="Megametre"/>
			<xsd:enumeration value="Nanometre"/>
			<xsd:enumeration value="Femtometre"/>
			<xsd:enumeration value="Fathom"/>
			<xsd:enumeration value="Mil"/>
			<xsd:enumeration value="Parsec"/>
			<xsd:enumeration value="Angstrom"/>
			<xsd:enumeration value="TenthOfAnInch"/>
			<xsd:enumeration value="NauticalMile"/>
			<xsd:enumeration value="16thOfAnInch"/>
			<xsd:enumeration value="32ndOfAnInch"/>
			<xsd:enumeration value="64thOfAnInch"/>
			<xsd:enumeration value="ImperialYard"/>
			<xsd:enumeration value="ImperialFoot"/>
			<xsd:enumeration value="UsSurveyInch"/>
			<xsd:enumeration value="UsSurveyFoot"/>
			<xsd:enumeration value="UsSurveyMile"/>
			<xsd:enumeration value="UsSurveyChain"/>
			<xsd:enumeration value="UsSurveyLink"/>
			<xsd:enumeration value="ClarkeChain"/>
			<xsd:enumeration value="ClarkeLink"/>
			<xsd:enumeration value="ClarkeYard"/>
			<xsd:enumeration value="SearsChain"/>
			<xsd:enumeration value="SearsLink"/>
			<xsd:enumeration value="SearsFoot"/>
			<xsd:enumeration value="SearsYard"/>
			<xsd:enumeration value="GermanLegalMetre"/>
			<xsd:enumeration value="GoldCoastFoot"/>
			<xsd:enumeration value="ModifiedAmericanFoot"/>
			<xsd:enumeration value="IndianGeodeticFoot"/>
			<xsd:enumeration value="IndianYard"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Basic Elements -->
	<xsd:element name="UnitsOfMeasure">
		<xsd:annotation>
			<xsd:documentation>These are from the ISO 15926 Reference Data Library (RDL) Units of measure. This Element dfefines the allowable sets of values for the various measurement units. This construct is not intended to allow parser validation of the values, it is intended for validation using XMpLant and also documentation. </xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="Distance" type="LengthUnitsType" default="Millimetre">
				<xsd:annotation>
					<xsd:documentation>These Distance units are from the ISO 15926 Reference Data Library (RDL) Units of measure </xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="Angle" default="Degree-angle">
				<xsd:annotation>
					<xsd:documentation>These angle units are from the ISO 15926 Reference Data Library (RDL) Units of measure </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="rad"/>
						<xsd:enumeration value="deg"/>
						<xsd:enumeration value="Radian"/>
						<xsd:enumeration value="Degree-angle"/>
						<xsd:enumeration value="CentesimalSecond"/>
						<xsd:enumeration value="CentesimalMinute"/>
						<xsd:enumeration value="Second-angle"/>
						<xsd:enumeration value="Minute-angle"/>
