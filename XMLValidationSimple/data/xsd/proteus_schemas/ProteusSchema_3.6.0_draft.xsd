				<xsd:annotation>
					<xsd:documentation>These Temperature units are from the ISO 15926 Reference Data Library (RDL) Units of measure </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="degC"/>
