						<xsd:enumeration value="Mil_6400Radian"/>
						<xsd:enumeration value="Cycle"/>
						<xsd:enumeration value="Iso2041Cycle"/>
						<xsd:enumeration value="DecimalDegree"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="Area" default="MetreSquared">
				<xsd:annotation>
					<xsd:documentation>These Area units are from the ISO 15926 Reference Data Library (RDL) Units of measure </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="sq mm"/>
						<xsd:enumeration value="sq cm"/>
						<xsd:enumeration value="sq dm"/>
						<xsd:enumeration value="sq m"/>
						<xsd:enumeration value="sq km"/>
						<xsd:enumeration value="sq in"/>
						<xsd:enumeration value="sq ft"/>
						<xsd:enumeration value="sq yd"/>
						<xsd:enumeration value="MillimetreSquared"/>
						<xsd:enumeration value="CentimetreSquared"/>
						<xsd:enumeration value="DecimetreSquared"/>
						<xsd:enumeration value="MetreSquared"/>
						<xsd:enumeration value="KilometreSquared"/>
						<xsd:enumeration value="InchSquared"/>
						<xsd:enumeration value="FootSquared"/>
						<xsd:enumeration value="YardSquared"/>
						<xsd:enumeration value="MileSquared"/>
						<xsd:enumeration value="Barn"/>
						<xsd:enumeration value="MicrometreSquared"/>
						<xsd:enumeration value="Are"/>
						<xsd:enumeration value="Hectare"/>
						<xsd:enumeration value="Acre"/>
						<xsd:enumeration value="HundredFootSquared"/>
						<xsd:enumeration value="UsSurveyMileSquared"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="Volume" default="MetreCubed">
				<xsd:annotation>
					<xsd:documentation>These Volume units are from the ISO 15926 Reference Data Library (RDL) Units of measure </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="cc"/>
						<xsd:enumeration value="MicrometreSquaredMetre"/>
						<xsd:enumeration value="MillimetreCubed"/>
						<xsd:enumeration value="CentimetreCubed"/>
						<xsd:enumeration value="DecimetreCubed"/>
						<xsd:enumeration value="MetreCubed"/>
						<xsd:enumeration value="StandardMetreCubed"/>
						<xsd:enumeration value="KilometreCubed"/>
						<xsd:enumeration value="MillionMetreCubed"/>
						<xsd:enumeration value="Millilitre"/>
						<xsd:enumeration value="Centilitre"/>
						<xsd:enumeration value="Decilitre"/>
						<xsd:enumeration value="Litre"/>
