						<xsd:enumeration value="AcreFoot"/>
						<xsd:enumeration value="UkBushel"/>
						<xsd:enumeration value="UkFluidOunce"/>
						<xsd:enumeration value="UkGallon"/>
						<xsd:enumeration value="ThousandUkGallon"/>
						<xsd:enumeration value="UkPint"/>
						<xsd:enumeration value="UkQuart"/>
						<xsd:enumeration value="UsBarrel"/>
						<xsd:enumeration value="UsBushel"/>
						<xsd:enumeration value="UsDryBarrel"/>
						<xsd:enumeration value="UsDryPint"/>
						<xsd:enumeration value="UsFluidOunce"/>
						<xsd:enumeration value="UsGallon"/>
